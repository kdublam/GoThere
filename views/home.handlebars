<div class="container">
  <div id="useGoThere">
    <h4>Getting started with GoThere</h4>

    <div class="row" id="userInput">

      <form class="col s12" action="/result" method="POST">

        <div class="row">
          <div class="input-field col s12">
            <input placeholder="Starting point" id="start" name="start" type="text" class="validate">
            <label for="start">Where is your starting point?</label>
          </div>
        </div>

        <div class="row">
          <div class="input-field col s12">
            <input placeholder="Destination" id="end" name="end" type="text" class="validate">
            <label for="end">Where are you going?</label>
          </div>
        </div>



        <div class="row">
          <div class="input-field col s12">
            <input id="calendar" name="calendar" onchange="checkDate()" required class="datepicker-input"
              placeholder="mm/dd/yyy" type="date" data-date-format="yyyy-mm-dd">
            <label for="calendar">Pick date</label>
          </div>
        </div>



        <div class="row">
          <div class="input-field col s12">
            <input placeholder="hh:mm" id="clock" name="clock" onchange="checkTime()" type="text" class="timepicker">
            <label for="time">Pick time(Arrival Time)</label>
          </div>
        </div>


        <div class="row">
          <div class="input-field col s12">
            <select id="transMethod" name="transMethod">
              <option value="" disabled selected>Choose your option</option>
              <option value="WALKING">Walking</option>
              <option value="DRIVING">Driving</option>
              <option value="TRANSIT">Transit</option>
              <option value="BICYCLING">Bike</option>
            </select>
            <label for="transMethod">Choose your transportaion</label>
          </div>
        </div>
        <input type="submit" class="waves-effect waves-light btn" id="submit">
      </form>
    </div>

  </div>

</div>
