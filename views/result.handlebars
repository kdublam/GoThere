{{!-- display matches --}}
<div class="container">
  <div class="row">
    <div class="col-8 offset-2">
      <div class="card mt-4">
        <div class="card-body">
          <h5 class="card-title">We found a match!</h5>
          <p>
            <strong>Name</strong>: {{user.name}}</p>
          <p>
            <p>
              <strong>Destination</strong>: {{plan.destination}}</p>
            <p>
              <strong>Arrival Date Time</strong> {{plan.arrivalDateTime}}</p>

            <p>
              <strong>Transportation:</strong> {{plan.transportation}}</p>
            <p>
              <strong>Contact:</strong> {{user.email}}</p>

        </div>
      </div>
    </div>
  </div>
</div>